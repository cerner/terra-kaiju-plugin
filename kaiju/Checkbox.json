{
  "name": "Checkbox",
  "library": "terra-form-checkbox",
  "display": "Checkbox",
  "description": "The Terra Form Checkbox is a responsive input component rendered as a box. When activated, a check mark shall appear. Focus can be activated through tabbing and the checked state can be toggled with the space bar.",
  "group": "Atoms::Forms",
  "documentation": "http://engineering.cerner.com/terra-ui/#/components/terra-form-checkbox/form-checkbox/form-checkbox",
  "properties": {
    "defaultChecked": {
      "type": "Bool"
    },
    "isInline": {
      "type": "Bool"
    },
    "disabled": {
      "type": "Bool"
    },
    "isLabelHidden": {
      "type": "Bool"
    },
    "labelText": {
      "type": "String",
      "display": "Label",
      "default": "Label"
    },
    "value": {
      "type": "String"
    },
    "id": {
      "type": "String",
      "display": "Id"
    },
    "name": {
      "type": "String",
      "display": "Name"
    }
  }
}
