{
  "name": "Image",
  "library": "terra-image",
  "display": "Image",
  "description": "The terra-image component provides styling for visual imagery.",
  "group": "Atoms::Other",
  "documentation": "http://engineering.cerner.com/terra-ui/#/components/terra-image/image/image",
  "properties": {
    "src": {
      "type": "String",
      "form_type": "DelayedInput",
      "default": "/Gamera_Maracas.gif"
    },
    "alt": {
      "type": "String",
      "default": "alt-text"
    },
    "height": {
      "type": "String"
    },
    "width": {
      "type": "String"
    },
    "isFluid": {
      "type": "Bool",
      "display": "Fluid"
    },
    "variant": {
      "type": "String",
      "form_type": "CodifiedList",
      "default": "default",
      "options": [
        {
          "display": "default",
          "value": "default",
          "type": "String"
        },
        {
          "display": "Rounded",
          "value": "rounded",
          "type": "String"
        },
        {
          "display": "Circle",
          "value": "circle",
          "type": "String"
        },
        {
          "display": "Thumbnail",
          "value": "thumbnail",
          "type": "String"
        }
      ]
    },
    "placeholder": {
      "type": "Component",
      "display": "Placeholder"
    }
  }
}
