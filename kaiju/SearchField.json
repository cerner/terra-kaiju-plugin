{
  "name": "SearchField",
  "library": "terra-search-field",
  "display": "Search Field",
  "description": "A search component with a field that automatically performs a search callback after user input.",
  "group": "Molecules::Other",
  "documentation": "http://engineering.cerner.com/terra-ui/#/components/terra-search-field/search-field",
  "properties": {
    "isBlock": {
      "type": "Bool",
      "display": "Block"
    },
    "isDisabled": {
      "type": "Bool",
      "display": "Disabled"
    },
    "disableAutoSearch": {
      "type": "Bool"
    },
    "placeholder": {
      "type": "String",
      "default": "Search..."
    },
    "minimumSearchTextLength": {
      "type": "Number",
      "default": 2
    },
    "searchDelay": {
      "type": "Number",
      "default": 250
    },
    "defaultValue": {
      "type": "String",
      "display": "Default value"
    }
  }
}
