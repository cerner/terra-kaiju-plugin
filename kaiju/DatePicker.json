{
  "name": "DatePicker",
  "library": "terra-date-picker",
  "display": "Date Picker",
  "description": "A controlled input component for selecting dates",
  "group": "Atoms::Forms",
  "documentation": "http://engineering.cerner.com/terra-ui/#/site/components/core/site/date-picker/index",
  "properties": {
    "isDisabled": {
      "type": "Bool",
      "display": "Disabled"
    },
    "excludeDates": {
      "type": "Array",
      "schema": {
        "type": "String"
      }
    },
    "includeDates": {
      "type": "Array",
      "schema": {
        "type": "String"
      }
    },
    "maxDate": {
      "type": "String"
    },
    "minDate": {
      "type": "String"
    },
    "name": {
      "type": "String",
      "default": "Date Picker"
    },
    "selectedDate": {
      "type": "String"
    }
  }
}
