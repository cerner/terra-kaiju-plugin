{
  "name": "DatePicker",
  "library": "terra-date-picker",
  "display": "Date Picker",
  "description": "The terra-date-picker component provides users a way to enter or select a date from the date picker.",
  "group": "Atoms::Forms",
  "documentation": "http://engineering.cerner.com/terra-ui/#/components/terra-date-picker/date-picker/date-picker",
  "properties": {
    "disabled": {
      "type": "Bool",
      "display": "Disabled"
    },
    "excludeDates": {
      "type": "Array",
      "schema": {
        "type": "String"
      }
    },
    "includeDates": {
      "type": "Array",
      "schema": {
        "type": "String"
      }
    },
    "maxDate": {
      "type": "String"
    },
    "minDate": {
      "type": "String"
    },
    "name": {
      "type": "String",
      "default": "Date Picker"
    },
    "selectedDate": {
      "type": "String"
    },
    "required": {
      "type": "Bool",
      "display": "Required"
    },
    "isIncomplete": {
      "type": "Bool",
      "display": "Incomplete"
    },
    "isInvalid": {
      "type": "Bool",
      "display": "Invalid"
    }
  }
}
