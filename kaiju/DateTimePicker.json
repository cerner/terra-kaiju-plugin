{
  "name": "DateTimePicker",
  "library": "terra-date-time-picker",
  "display": "Date Time Picker",
  "description": "The DateTimePicker component has a date picker for selecting date and a time input for entering time",
  "group": "Atoms::Forms",
  "documentation": "http://engineering.cerner.com/terra-ui/#/components/terra-date-time-picker/date-time-picker/date-time-picker",
  "properties": {
    "disabled": {
      "type": "Bool"
    },
    "excludeDates": {
      "type": "Array",
      "schema": {
        "type": "String"
      }
    },
    "includeDates": {
      "type": "Array",
      "schema": {
        "type": "String"
      }
    },
    "name": {
      "type": "String",
      "default": "Date Time Picker"
    },
    "value": {
      "type": "String"
    },
    "maxDate": {
      "type": "String",
      "display": "Max date"
    },
    "minDate": {
      "type": "String",
      "display": "Min date"
    },
    "showSeconds": {
      "type": "Bool",
      "display": "Show seconds"
    },
    "isIncomplete": {
      "type": "Bool",
      "display": "Incomplete"
    },
    "isInvalid": {
      "type": "Bool",
      "display": "Invalid"
    },
    "isInvalidMeridiem": {
      "type": "Bool",
      "display": "Invalid meridiem"
    },
    "required": {
      "type": "Bool",
      "display": "Required"
    }
  }
}
