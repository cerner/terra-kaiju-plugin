{
  "name": "SelectField",
  "library": "terra-form-select",
  "display": "Select field",
  "description": "A select input wrapped in a field component.",
  "group": "Molecules::Forms",
  "documentation": "http://engineering.cerner.com/terra-ui/#/components/terra-form-select/form-select/form-select",
  "properties": {
    "hideRequired": {
      "type": "Bool",
      "display": "Hide required"
    },
    "isInline": {
      "type": "Bool",
      "display": "Inline"
    },
    "isInvalid": {
      "type": "Bool",
      "display": "Invalid"
    },
    "isLabelHidden": {
      "type": "Bool",
      "display": "Label hidden"
    },
    "showOptional": {
      "type": "Bool",
      "display": "Show optional"
    },
    "required": {
      "type": "Bool",
      "display": "Required"
    },
    "label": {
      "type": "Array",
      "display": "Label",
      "schema": {
        "type": "Component"
      },
      "default": [
        {
          "type": "kaiju::Text",
          "props": {
            "text": "Label"
          }
        }
      ]
    },
    "error": {
      "type": "Array",
      "display": "Error",
      "schema": {
        "type": "Component"
      }
    },
    "help": {
      "type": "Array",
      "display": "Help",
      "schema": {
        "type": "Component"
      }
    },
    "selectId": {
      "type": "String",
      "display": "Select id"
    },
    "placeholder": {
      "type": "String",
      "display": "Placeholder"
    },
    "variant": {
      "type": "String",
      "form_type": "CodifiedList",
      "default": "default",
      "options": [
        {
          "display": "Default",
          "value": "default",
          "type": "String"
        },
        {
          "display": "Search",
          "value": "search",
          "type": "String"
        },
        {
          "display": "Combobox",
          "value": "combobox",
          "type": "String"
        },
        {
          "display": "Multiple",
          "value": "multiple",
          "type": "String"
        },
        {
          "display": "Tag",
          "value": "tag",
          "type": "String"
        }
      ]
    },
    "children": {
      "type": "Array",
      "schema": {
        "type": "Component"
      },
      "default": [
        {
          "type": "terra-form-select::Option",
          "props": {
            "display": "Apple",
            "value": "apple"
          }
        },
        {
          "type": "terra-form-select::Option",
          "props": {
            "display": "Pear",
            "value": "pear"
          }
        },
        {
          "type": "terra-form-select::Option",
          "props": {
            "display": "Orange",
            "value": "orange"
          }
        }
      ]
    },
    "maxWidth": {
      "type": "String",
      "display": "Max width"
    },
    "allowClear": {
      "type": "Bool",
      "display": "Allow clear"
    },
    "maxSelectionCount": {
      "type": "Number",
      "display": "Max selection count"
    },
    "disabled": {
      "type": "Bool",
      "display": "Disabled"
    },
    "isTouchAccessible": {
      "type": "Bool",
      "display": "Touch accessible"
    },
    "isIncomplete": {
      "type": "Bool",
      "display": "Incomplete"
    }
  },
  "import_from": "terra-form-select/lib/SelectField"
}
